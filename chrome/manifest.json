{
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Purple AdBlock",
        "default_icon": {
            "128": "images/logov2-128.png"
        },
        "default_popup": "common/html/popup.html"
    },
    "icons": {
        "128": "images/logov2-128.png"
    },
    "description": "Purple AdBlock",
    "manifest_version": 2,
    "name" : "Purple AdBlock",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://*.twitch.tv/*",
        "https://much.ga/*",
        "https://*.ttvnw.net/*",
        "activeTab",
        "storage",
        "https://*.amazon-adsystem.com/*",
        "https://cdn.krxd.net/*",
        "https://script.ioam.de/iam.js",
        "https://edge.quantserve.com/quant.js",
        "https://ddacn6pr5v0tl.cloudfront.net/custom-moat-video-native.min.js",
        "https://d2v02itv0y9u9t.cloudfront.net/dist/1.0.5/v6s.js",
        "https://*.imrworldwide.com/*",
        "https://countess.twitch.tv/*",
        "https://*.scorecardresearch.com/*",
        "https://www.googletagservices.com/tag/js/gpt.js",
        "*://*.branch.io/*",
        "*://comscore.com/*"
    ],
    "version" : "1.5.6"
}