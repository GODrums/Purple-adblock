    "dev": "cross-env NODE_ENV=development nodemon"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express-pino-logger": "^4.0.3",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "csurf": "^1.11.0",
    "dotenv": "^14.0.1",
    "express": "^4.17.1",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.17.1",
    "helmet": "^5.0.1",
    "https-proxy-agent": "^5.0.0",
    "ping": "^0.4.1",
    "pino": "^7.6.3",
    "pino-pretty": "^7.3.0"
  }
