{
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Purple Ads Blocker",
    "default_icon": "images/logov2-128.png",
    "default_popup": "common/html/popup.html"
  },
  "icons": {
    "128": "images/logov2-128.png"
  },
  "description": "Purple AdBlocker is an ad-blocker for Twitch",
  "manifest_version": 2,
  "name": "Purple Ads Blocker",
  "version": "1.5.0.2",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.twitch.tv/*",
    "https://usher.ttvnw.net/*",
    "https://much.ga/*",
    "http://127.0.0.1:8080/*",
    "tabs",
    "activeTab",
    "storage"
  ]
}
